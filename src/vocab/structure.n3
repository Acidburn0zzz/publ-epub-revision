@base  <http://www.idpf.org/epub/vocab/structure/> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix zt:  <http://www.daisy.org/ns/rdf/property#> .
@prefix htu: <http://www.daisy.org/ns/rdf/usage/html/#> .
@prefix mou: <http://www.daisy.org/ns/rdf/usage/media-overlays/#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix jena: <http://www.daisy.org/ns/rdf/jena#> .
@prefix foaf: <http://xmlns.com/foaf/spec/#term_> .
@prefix aria: <http://www.w3.org/WAI/ARIA/schemata/aria-1.rdf#> .
@prefix xhv: <http://www.w3.org/1999/xhtml/vocab/#> .
@prefix role: <http://www.w3.org/1999/xhtml/vocab#> .
@prefix epub: <http://www.idpf.org/epub/vocab#> .
@prefix : <#> .

<>
  dcterms:title "EPUB 3 Structural Semantics Vocabulary" ;
  dcterms:description "This vocabulary, a part of EPUBÂ® 3, defines a set of properties relating to the description of structural semantics of written works. While the vocabulary is generally host language agnostic, it has been constructed primarily to enable semantic inflection of elements in the HTML vocabulary." ;
  dcterms:publisher "International Digital Publishing Forum (IDPF)" ;
  dcterms:issued "2014-06-28" ; 
  dcterms:creator "mgylling" ; 
  dcterms:creator "mgarrish" .
  

########## partitions #########

:partitions                                a rdf:Bag ;
					                       jena:bag-sort "10" ;
					                       dcterms:title "Document partitions" .
					
:cover                                     a rdf:Property ;
					                       rdfs:label "cover";
					                       rdf:member :partitions ;
					                       htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
					                       jena:prop-sort "10" ;
					                       rdfs:comment "A section that introduces the work, often consisting of a marketing image, the title, author and publisher, and select quotes and reviews." .							

:frontmatter                               a rdf:Property ;
					                       rdfs:label "frontmatter";
					                       rdf:member :partitions ;
					                       htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
					                       jena:prop-sort "20" ;
					                       rdfs:comment "Preliminary material to the main content of a publication, such as tables of contents, dedications, etc." .

:bodymatter                                a rdf:Property ;
					                       rdfs:label "bodymatter";
					                       rdf:member :partitions ;
					                       htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
					                       jena:prop-sort "30" ;
					                       rdfs:comment "The main content of a publication." .

:backmatter                                a rdf:Property ;
					                       rdfs:label "backmatter";
					                       rdf:member :partitions ;
					                       htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
					                       jena:prop-sort "40" ;
					                       rdfs:comment "Ancillary material occurring after the main content of a publication, such as indices, appendices, etc." .



########## divisions #########

:divisions                                  a rdf:Bag ;
                                            jena:bag-sort "20" ;
                                            dcterms:title "Document divisions" .

:volume                                     a rdf:Property ;
                                            rdfs:label "volume";
                                            rdf:member :divisions ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            jena:prop-sort "10" ;
                                            rdfs:comment "A component of a collection." .

:part                                       a rdf:Property;
                                            rdfs:label "part";
                                            rdf:member :divisions ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            jena:prop-sort "20" ;
                                            rdfs:comment "A major structural division in a work that contains a set of related sections dealing with a particular subject, narrative arc or similar encapsulated theme." .

:chapter                                    a rdf:Property ;
					                        rdfs:label "chapter";
                                            rdf:member :divisions ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            jena:prop-sort "30" ;
                                            rdfs:comment "A major thematic section of content in a work." .

:subchapter                                 a rdf:Property ;
                                            rdfs:label "subchapter";
                                            rdf:member :divisions ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            jena:prop-sort "40" ;
                                            rdfs:comment "A major sub-division of a chapter." .

:division                                   a rdf:Property ;
                                            rdfs:label "division";
                                            rdf:member :divisions ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            jena:prop-sort "50" ;
                                            rdfs:comment "A major structural division that may also appear as a substructure of a part (esp. in legislation)." .

#:section                                   a rdf:Property ;
#                                           rdfs:label "section";
#                                           rdf:member :divisions ;
#                                           rdfs:comment "A structural division typically subordinate in importance to a part or division (esp. in textbooks and legislation)." .
#
#:subsection                                a rdf:Property ;
#                                           rdfs:label "subsection";
#                                           rdf:member :divisions ;
#                                           rdfs:comment "A major sub-division of a section." .


########## sections #########

:sections                                   a rdf:Bag ;
					                        jena:bag-sort "30" ;
					                        dcterms:title "Document sections and components" ;
					                        dcterms:description "Sections and components that typically occur in the publication bodymatter." .

:abstract                                   a rdf:Property ;
                                            rdfs:label "abstract";
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "5" ;
                                            rdfs:comment "A short summary of the principle ideas, concepts and conclusions of the work, or of a section or excerpt within it." ;
                                            epub:status "draft" .

:foreword                                   a rdf:Property ;
                                            rdfs:label "foreword";
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "10" ;
                                            rdfs:comment "An introductory section that precedes the work, typically not written by the author of the work." .

:preface                                    a rdf:Property ;
                                            rdfs:label "preface";
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "20" ;
                                            rdfs:comment "An introductory section that precedes the work, typically written by the author of the work." .

:prologue                                   a rdf:Property ;
                                            rdfs:label "prologue" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "30" ;
                                            rdfs:comment "An introductory section that sets the background to a work, typically part of the narrative." .

:introduction                               a rdf:Property ;
                                            rdfs:label "introduction";
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "40" ;
                                            rdfs:comment "A preliminary section that typically introduces the scope or nature of the work.".

:preamble                                   a rdf:Property ;
                                            rdfs:label "preamble" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "50" ;
                                            rdfs:comment "A section in the beginning of the work, typically containing introductory and/or explanatory prose regarding the scope or nature of the work's content".

:qna                                        a rdf:Property ;
                                            rdfs:label "qna" ;
                                            rdf:member :testing ;
                                            htu:usage "lists or [[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "70" ;
                                            rdfs:comment "A section of content structured as a series of questions and answers, such as an interview or list of frequently asked questions." .

:conclusion                                 a rdf:Property ;
                                            rdfs:label "conclusion" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "60" ;
                                            rdfs:comment "A concluding section or statement that summarizes the work or wraps up the narrative." .

:epilogue                                   a rdf:Property ;
                                            rdfs:label "epilogue" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "70" ;
                                            rdfs:comment "A concluding section of narrative that wraps up or comments on the actions and events of the work, typically from a future perspective." .

:afterword		                            a rdf:Property ;
                                            rdfs:label "afterword" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "80" ;
                                            rdfs:comment "A closing statement from the author or a person of importance, typically providing insight into how the content came to be written, its significance, or related events that have transpired since its timeline." .

:epigraph                                   a rdf:Property ;
                                            rdfs:label "epigraph" ;
                                            rdf:member :sections ;
                                            htu:usage "[[http://www.w3.org/TR/html5/sections.html#the-section-element section]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "90" ;
                                            rdfs:comment "A quotation set at the start of the work or a section that establishes the theme or sets the mood." .



########### navigation ###########

:navigation                                 a rdf:Bag ;
                                            jena:bag-sort "35" ;
                                            dcterms:title "Document navigation" .


:toc                                        a rdf:Property ;
                                            rdfs:label "toc" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "10" ;
                                            rdfs:comment "A navigational aid that provides an ordered list of links to the major sectional headings in the content. A table of contents may cover an entire work, or only a smaller section of it." .

:toc-brief                                  a rdf:Property ;
                                            rdfs:label "toc-brief" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "20" ;
                                            rdfs:comment "An abridged version of the table of contents." ;
                                            epub:status "draft" .

:landmarks                                  a rdf:Property ;
                                            rdfs:label "landmarks" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "25" ;
                                            rdfs:comment "A collection of references to well-known/recurring components within the publication" .

:loa                                        a rdf:Property ;
                                            rdfs:label "loa" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "30" ;
                                            rdfs:comment "A listing of audio clips included in the work." .

:loi                                        a rdf:Property ;
                                            rdfs:label "loi" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "40" ;
                                            rdfs:comment "A listing of illustrations included in the work." .

:lot                                        a rdf:Property ;
                                            rdfs:label "lot" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "50" ;
                                            rdfs:comment "A listing of tables included in the work." .

:lov                                        a rdf:Property ;
                                            rdfs:label "lov" ;
                                            rdf:member :navigation ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "60" ;
                                            rdfs:comment "A listing of video clips included in the work." .



########## reference sections #########


:references                                 a rdf:Bag ;
                                            jena:bag-sort "40" ;
                                            dcterms:title "Document reference sections" .

:appendix                                   a rdf:Property ;
                                            rdfs:label "appendix" ;
                                            rdf:member :references ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            jena:prop-sort "10" ;
                                            rdfs:comment "A section of supplemental information located after the primary content that informs the content but is not central to it." .

:colophon                                   a rdf:Property ;
                                            rdfs:label "colophon" ;
                                            rdf:member :references ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            jena:prop-sort "20" ;
                                            rdfs:comment "A short section of production notes particular to the edition (e.g., describing the typeface used), often located at the end of a work." .

:credits                                    a rdf:Property ;
                                            rdfs:label "credits" ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            rdf:member :references ;
                                            jena:prop-sort "30" ;
                                            rdfs:comment "A collection of [[#credit credits]]." ;
                                            epub:status "draft" .

:keywords                                   a rdf:Property ;
                                            rdfs:label "keywords" ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/grouping-content.html#grouping-content grouping content]]" ;
                                            rdf:member :references ;
                                            jena:prop-sort "40" ;
                                            rdfs:comment "A collection of [[#keyword keywords]]."  ;
                                            epub:status "draft" .


########### indexes ###########

:indexes                                    a rdf:Bag ;
                                            jena:bag-sort "41" ;
                                            rdf:member :references ;
                                            dcterms:title "Indexes" .


:index                                      a rdf:Property ;
                                            jena:prop-sort "10" ;
                                            rdfs:label "index" ;
                                            rdf:member :indexes ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/sections.html#the-body-element-0 body]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-index EPUB Indexes â index property]]" ;
                                            rdfs:comment "A navigational aid that provides a detailed list of links to key subjects, names and other important topics covered in the work." .

:index-headnotes                            a rdf:Property ;
                                            jena:prop-sort "20" ;
                                            rdfs:label "index-headnotes" ;
                                            rdf:member :indexes ;
                                            role:scope :index ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/sections.html#the-header-element header]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-headnotes EPUB Indexes â index-headnotes property]]" ;
                                            rdfs:comment "Narrative or other content to assist users in using the index." .

:index-legend                               a rdf:Property ;
                                            jena:prop-sort "30" ;
                                            rdfs:label "index-legend" ;
                                            rdf:member :indexes ;
                                            role:scope :index-headnotes ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]], [[http://www.w3.org/TR/html5/grouping-content.html#the-dl-element dl]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-legend EPUB Indexes â index-legend property]]" ;
                                            rdfs:comment "List of symbols, abbreviations or special formatting used in the index, and their meanings." .

:index-group                                a rdf:Property ;
                                            jena:prop-sort "40" ;
                                            rdfs:label "index-group" ;
                                            rdf:member :indexes ;
                                            role:scope :index ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#sectioning-content-0 sectioning content]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-group EPUB Indexes â index-group property]]" ;
                                            rdfs:comment "Collection of consecutive main entries that share a common characteristic, for example the starting letter of the main entries." .

:index-entry-list                           a rdf:Property ;
                                            jena:prop-sort "50" ;
                                            rdfs:label "index-entry-list" ;
                                            rdf:member :indexes ;
                                            role:scope :index ;
                                            role:scope :index-group ;
                                            role:scope :index-entry ;
                                            htu:usage "[[http://www.w3.org/TR/html5/grouping-content.html#the-ul-element ul]]; this property is implied when the ul has an ancestor of index except within index-headnotes" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-entry-list EPUB Indexes â index-entry-list property]]" ;
                                            rdfs:comment "Collection of consecutive main entries or subentries." .

:index-entry                                a rdf:Property ;
                                            jena:prop-sort "60" ;
                                            rdfs:label "index-entry" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry-list ;
                                            htu:usage "[[http://www.w3.org/TR/html5/grouping-content.html#the-li-element li]]; this property is implied when parent ul is of type index-entry-list (implicit or explicit)" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-entry EPUB Indexes â index-entry property]]" ;
                                            rdfs:comment "One term with any attendant subentries, locators, cross references, and/or editorial note." .

:index-term                                 a rdf:Property ;
                                            jena:prop-sort "70" ;
                                            rdfs:label "index-term" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            role:scope :index-xref-preferred ;
                                            role:scope :index-xref-related ;
                                            htu:usage "[[http://www.w3.org/TR/html5/dom.html#phrasing-content phrasing content]]; typically [[http://www.w3.org/TR/html5/text-level-semantics.html#the-span-element span]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-term EPUB Indexes â index-term property]]" ;
                                            rdfs:comment "Word, phrase, string, glyph or image representing the indexable content." .

:index-editor-note                          a rdf:Property ;
                                            jena:prop-sort "80" ;
                                            rdfs:label "index-editor-note" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            htu:usage "[[http://www.w3.org/TR/html5/rendering.html#flow-content-0 flow content]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-editor-note EPUB Indexes â index-editor-note property]]" ;
                                            rdfs:comment "Editorial note pertaining to a single entry." .

:index-locator                              a rdf:Property ;
                                            jena:prop-sort "90" ;
                                            rdfs:label "index-locator" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            role:scope :index-locator-list ;
                                            role:scope :index-locator-range ;
                                            htu:usage "[[http://www.w3.org/TR/html5/text-level-semantics.html#the-a-element a]]; this property is implied when parent context is index-locator-list or index-locator-range" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-locator EPUB Indexes â index-locator property]]" ;
                                            rdfs:comment "A reference to an occurrence of the indexed content in the publication." .

:index-locator-list                         a rdf:Property ;
                                            jena:prop-sort "100" ;
                                            rdfs:label "index-locator-list" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            htu:usage "[[http://www.w3.org/TR/html5/grouping-content.html#the-ul-element ul]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-locator-list EPUB Indexes â index-locator-list property]]" ;
                                            rdfs:comment "Collection of sequential locators or locator ranges." .

:index-locator-range                        a rdf:Property ;
                                            jena:prop-sort "110" ;
                                            rdfs:label "index-locator-range" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            role:scope :index-locator-list ;
                                            htu:usage "[[http://www.w3.org/TR/html5/rendering.html#flow-content-0 flow content]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-locator-range EPUB Indexes â index-locator-range property]]" ;
                                            rdfs:comment "A pair of locators that connects a term to a range of content rather than a single point." .

:index-xref-preferred                       a rdf:Property ;
                                            jena:prop-sort "120" ;
                                            rdfs:label "index-xref-preferred" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            htu:usage "[[http://www.w3.org/TR/html5/rendering.html#flow-content-0 flow content]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-xref-preferred EPUB Indexes â index-xref-preferred property]]" ;
                                            rdfs:comment "Reference from one term to one or more preferred terms or term categories in an index (analogous to \"See xxx\")." .

:index-xref-related                         a rdf:Property ;
                                            jena:prop-sort "130" ;
                                            rdfs:label "index-xref-related" ;
                                            rdf:member :indexes ;
                                            role:scope :index-entry ;
                                            htu:usage "[[http://www.w3.org/TR/html5/rendering.html#flow-content-0 flow content]]" ;
                                            epub:usage "[[http://www.idpf.org/epub/idx/epub-indexes.html#bb-xref-related EPUB Indexes â index-xref-related property]]" ;
                                            rdfs:comment "Reference from one term to one or more related terms or term categories in an index (analogous to \"See also xxx\")." .

:index-term-category                        a rdf:Property ;
                                            jena:prop-sort "140" ;
                                            rdfs:label "index-term-category" ;
                                            rdf:member :indexes ;
